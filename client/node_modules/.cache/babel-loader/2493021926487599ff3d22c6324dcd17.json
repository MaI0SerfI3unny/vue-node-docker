{"ast":null,"code":"import Header from '../../components/Header.vue';\nimport Footer from '../../components/Footer.vue';\nimport axios from 'axios';\nexport default {\n  name: 'SingleComponent',\n  components: {\n    Header,\n    Footer\n  },\n\n  data() {\n    return {\n      content: [],\n      email: null\n    };\n  },\n\n  mounted() {\n    if (localStorage.email) {\n      this.email = localStorage.email;\n    } else {\n      this.$router.push(\"/\");\n    }\n\n    axios.post('/api/auth/find_by_id', {\n      id: this.$route.params.id\n    }).then(res => {\n      if (res.status === 200) {\n        this.content = res.data.result[0];\n      } else {\n        this.$router.push(\"/\");\n      }\n    }).catch(() => {\n      this.$router.push(\"/\");\n    });\n  },\n\n  methods: {\n    editBlog() {\n      axios.post('/api/auth/edit_blog', {\n        id: this.$route.params.id,\n        title: this.content.title,\n        description: this.content.description,\n        content: this.content.content\n      }).then(res => {\n        if (res.data.result === 'OK') {\n          this.$router.push(\"/admin\");\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuBA,OAAOA,MAAP,MAAmB,6BAAnB;AACA,OAAOC,MAAP,MAAmB,6BAAnB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,iBADO;AAEbC,YAAU,EAAE;AACVJ,UADU;AAEVC;AAFU,GAFC;;AAMbI,MAAI,GAAG;AACL,WAAO;AACLC,aAAO,EAAE,EADJ;AAELC,WAAK,EAAE;AAFF,KAAP;AAID,GAXY;;AAafC,SAAO,GAAG;AACL,QAAGC,YAAY,CAACF,KAAhB,EAAsB;AACnB,WAAKA,KAAL,GAAaE,YAAY,CAACF,KAA1B;AACF,KAFD,MAEK;AACF,WAAKG,OAAL,CAAaC,IAAb,CAAkB,GAAlB;AACH;;AACDT,SAAK,CAACU,IAAN,CAAW,sBAAX,EACA;AAACC,QAAE,EAAE,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF;AAAxB,KADA,EAECG,IAFD,CAEOC,GAAD,IAAQ;AACX,UAAGA,GAAG,CAACC,MAAJ,KAAe,GAAlB,EAAsB;AAClB,aAAKZ,OAAL,GAAeW,GAAG,CAACZ,IAAJ,CAASc,MAAT,CAAgB,CAAhB,CAAf;AACH,OAFD,MAEK;AACA,aAAKT,OAAL,CAAaC,IAAb,CAAkB,GAAlB;AACL;AACF,KARD,EAQGS,KARH,CAQS,MAAM;AACV,WAAKV,OAAL,CAAaC,IAAb,CAAkB,GAAlB;AACJ,KAVD;AAWC,GA9BU;;AA+BbU,SAAO,EAAC;AACJC,YAAQ,GAAE;