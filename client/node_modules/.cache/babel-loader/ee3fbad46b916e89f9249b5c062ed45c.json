:{\n      editBlog(){\n          axios.post('http://localhost:5555/api/auth/edit_blog',\n          {\n              title: this.title,\n              description: this.description,\n              content: this.contentBlog,\n          }).then((res) => {\n              console.log(res);\n          })\n      }\n  }\n}\n</script>\n\n<style>\n.form_edit input{\n    width: 320px;\n    height: 35px;\n    border: 1px solid grey;\n    border-radius: 5px;\n    padding-left: 10px;\n    margin-top: 5px;\n    margin-bottom: 10px;\n}\n\n.form_edit textarea{\n    width: 520px;\n    padding:5px;\n    resize: none;\n    height: 200px;\n}\n\n\n.form_edit button{\n    width: 100px;\n    border-radius: 5px;\n    background: green;\n    color: white;\n    height: 35px;\n    transition: 0.5s;\n    cursor: pointer;\n    border: none;\n    margin-top: 10px;\n}\n\n.form_edit button:hover{\n    background: black;\n}\n</style>"]},"metadata":{},"sourceType":"module"}