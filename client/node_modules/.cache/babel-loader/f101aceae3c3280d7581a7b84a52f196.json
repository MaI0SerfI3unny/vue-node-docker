{"ast":null,"code":"import Header from '../../components/Header.vue';\nimport Sidebar from '../../components/Sidebar.vue';\nimport Footer from '../../components/Footer.vue';\nexport default {\n  name: 'LoginComponent',\n  components: {\n    Header,\n    Sidebar,\n    Footer\n  },\n\n  data() {\n    return {\n      email: \"\",\n      password: \"\"\n    };\n  },\n\n  methods: {\n    sendLogin() {\n      if (this.email.length !== 0 && this.password.length !== 0 && this.repeatPass.length !== 0 && this.repeatPass === this.password) {\n        axios.post('http://localhost:5555/api/auth/register', {\n          email: this.email,\n          password: this.password\n        }).then(response => {\n          if (response.status === 201) {\n            this.email = '';\n            this.password = '';\n            this.repeatPass = '';\n            this.$router.push(\"/login\");\n          }\n        });\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8BA,OAAOA,MAAP,MAAmB,6BAAnB;AACA,OAAOC,OAAP,MAAoB,8BAApB;AACA,OAAOC,MAAP,MAAmB,6BAAnB;AAEA,eAAe;AACbC,MAAI,EAAE,gBADO;AAEbC,YAAU,EAAE;AACVJ,UADU;AAEVC,WAFU;AAGVC;AAHU,GAFC;;AAOXG,MAAI,GAAG;AACP,WAAO;AACHC,WAAK,EAAC,EADH;AAEHC,cAAQ,EAAC;AAFN,KAAP;AAID,GAZY;;AAaXC,SAAO,EAAC;AACNC,aAAS,GAAE;AACP,UACI,KAAKH,KAAL,CAAWI,MAAX,KAAsB,CAAtB,IACA,KAAKH,QAAL,CAAcG,MAAd,KAAyB,CADzB,IAEA,KAAKC,UAAL,CAAgBD,MAAhB,KAA2B,CAF3B,IAGA,KAAKC,UAAL,KAAoB,KAAKJ,QAJ7B,EAIsC;AACjCK,aAAK,CAACC,IAAN,CAAW,yCAAX,EACA;AACIP,eAAK,EAAE,KAAKA,KADhB;AAEIC,kBAAQ,EAAE,KAAKA;AAFnB,SADA,EAKEO,IALF,CAKQC,QAAD,IAAc;AAChB,cAAGA,QAAQ,CAACC,MAAT,KAAoB,GAAvB,EAA2B;AACvB,iBAAKV,KAAL,GAAa,EAAb;AACA,iBAAKC,QAAL,GAAgB,EAAhB;AACA,iBAAKI,UAAL,GAAkB,EAAlB;AACA,iBAAKM,OAAL,CAAaC,IAAb,CAAkB,QAAlB;AACJ;AACH,SAZF;AAaL;AACJ;;AArBM;AAbG,CAAf","names":["Header","Sidebar","Footer","name","components","data","email","password","methods","sendLogin","length","repeatPass","axios","post","then","response","status","$router","push"],"sourceRoot":"","sources":["/Users/daniledkin/Do