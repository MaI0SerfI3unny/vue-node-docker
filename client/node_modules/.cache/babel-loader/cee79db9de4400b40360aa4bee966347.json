{"ast":null,"code":"import Header from '../../components/Header.vue';\nimport Sidebar from '../../components/Sidebar.vue';\nimport Footer from '../../components/Footer.vue';\nimport axios from 'axios';\nexport default {\n  name: 'MainComponent',\n  components: {\n    Header,\n    Sidebar,\n    Footer\n  },\n\n  data() {\n    return {\n      blogs: [],\n      total: 6,\n      pageNumber: 0\n    };\n  },\n\n  mounted() {\n    if (localStorage.email) this.email = localStorage.email;\n    axios.post('http://localhost:5555/api/auth/blog').then(response => {\n      this.blogs = response.data.blog;\n    });\n  },\n\n  computed: {\n    pageCount() {\n      let l = this.blogs.length,\n          s = this.total;\n      return Math.ceil(l / s);\n    },\n\n    paginatedData() {\n      const start = this.pageNumber * this.total,\n            end = start + this.total;\n      return this.blogs.slice(start, end);\n    }\n\n  },\n  methods: {\n    nextPage() {\n      this.pageNumber++;\n    },\n\n    prevPage() {\n      this.pageNumber--;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkCA,OAAOA,MAAP,MAAmB,6BAAnB;AACA,OAAOC,OAAP,MAAoB,8BAApB;AACA,OAAOC,MAAP,MAAmB,6BAAnB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,MAAI,EAAE,eADO;AAEbC,YAAU,EAAE;AACVL,UADU;AAEVC,WAFU;AAGVC;AAHU,GAFC;;AAObI,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,EADF;AAELC,WAAK,EAAE,CAFF;AAGLC,gBAAU,EAAE;AAHP,KAAP;AAKD,GAbY;;AAcbC,SAAO,GAAG;AACP,QAAGC,YAAY,CAACC,KAAhB,EAAuB,KAAKA,KAAL,GAAaD,YAAY,CAACC,KAA1B;AAExBT,SAAK,CAACU,IAAN,CAAW,qCAAX,EACGC,IADH,CACSC,QAAD,IAAc;AAClB,WAAKR,KAAL,GAAaQ,QAAQ,CAACT,IAAT,CAAcU,IAA3B;AACD,KAHH;AAKD,GAtBY;;AAuBbC,UAAQ,EAAC;AACPC,aAAS,GAAE;AACT,UAAIC,IAAI,KAAKZ,KAAL,CAAWa,MAAnB;AAAA,UACIC,IAAI,KAAKb,KADb;AAEA,aAAOc,IAAI,CAACC,IAAL,CAAUJ,CAAC,GAACE,CAAZ,CAAP;AACD,KALM;;AAMPG,iBAAa,GAAE;AACf,YAAMC,KAAI,GAAI,KAAKhB,UAAL,GAAkB,KAAKD,KAArC;AAAA,YACMkB,GAAE,GAAID,KAAI,GAAI,KAAKjB,KADzB;AAGA,aAAO,KAAKD,KAAL,CAAWoB,KAAX,CAAiBF,KAAjB,EAAwBC,GAAxB,CAAP;AACA;;AAXO,GAvBI;AAoCbE,SAAO,EAAC;AACJC,YAAQ,